# vim:set ft=sshconfig:
# requires netcat for the proxy connections
Host *
  AddKeysToAgent yes
  Compression yes
  ControlMaster auto
  ControlPath ~/.ssh/socket-%r@%h:%p
  ControlPersist yes
  ForwardAgent yes
  PermitLocalCommand no
  PreferredAuthentications publickey,keyboard-interactive,password
  ServerAliveCountMax 20
  ServerAliveInterval 20

Host live
  HostName cultureofreason.org
  User ariuser

Host cor
  HostName cultureofreason.org
  User ariuser

Host bb
  HostName bitbucket.org
  User hg
  ForwardAgent no

Host github
  HostName github.com
  User git
  ForwardAgent no

Host demo
  HostName demo.aynrand.org
  User darceneaux

Host local
  User darc
  HostName localhost

Host personal-git
  HostName centuria-guild.org
  Port 32323
  User git
  ForwardAgent no

Host centuria
  HostName centuria-guild.org
  Port 23232
  User darc

Host aynrand2
  HostName aynrand2.org
  Port 32323
  User darc

Host limstest
  User limsadmin
  HostName 172.16.3.32
  #HostName 172.16.3.94

Host davidev
  User limsadmin
  HostName 172.16.3.105

Host davidmysql
  User limsadmin
