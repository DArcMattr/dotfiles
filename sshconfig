# vim:set ft=sshconfig:
# requires netcat for the proxy connections
Host *
  AddKeysToAgent yes
  AddressFamily inet
  CheckHostIP no
  Compression yes
  ControlMaster auto
  ControlPath ~/.ssh/socket-%r@%h:%p
  ForwardAgent yes
  PermitLocalCommand no
  PreferredAuthentications publickey,keyboard-interactive,password
  ServerAliveCountMax 20
  ServerAliveInterval 20

Host live
  HostName cultureofreason.org
  User ariuser

Host cor
  HostName cultureofreason.org
  User ariuser

Host bb
  HostName bitbucket.org
  User hg

Host github
  HostName github.com
  User git

Host demo
  HostName demo.aynrand.org
  User darceneaux

Host local
  User darc
  HostName localhost

Host personal-git
  HostName centuria-guild.org
  Port 32323
  User git

Host centuria
  HostName centuria-guild.org
  Port 23232
  User darc

Host aynrand2
  HostName aynrand2.org
  Port 32323
  User darc

Host vagrant
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentitiesOnly yes
  LogLevel FATAL
  ForwardAgent yes
  IdentityFile C:/Users/david.arceneaux/workspace/vagrant-fork/.vagrant/machines/default/virtualbox/private_key

Host staging
  HostName 10.5.32.72
  User ec2-user
  IdentityFile ~/.ssh/ten-staging.pem

Host preprod
  HostName 10.21.21.40
  User ec2-user
  IdentityFile ~/.ssh/ten-preprod.pem
