# vim:set ft=sshconfig:
# requires netcat for the proxy connections
Host *
  AddKeysToAgent yes
  AddressFamily inet
  Compression yes
  ControlMaster auto
  ControlPath ~/.ssh/socket-%r@%h:%p
  ControlPersist yes
  PermitLocalCommand no
  PreferredAuthentications publickey,keyboard-interactive,password
  ServerAliveCountMax 20
  ServerAliveInterval 20

Host live
  HostName cultureofreason.org
  User ariuser
  ForwardAgent yes

Host cor
  HostName cultureofreason.org
  User ariuser
  ForwardAgent yes

Host bb
  HostName bitbucket.org
  User hg

Host github
  HostName github.com
  User git

Host demo
  HostName demo.aynrand.org
  User darceneaux
  ForwardAgent yes

Host local
  User darc
  HostName localhost
  ForwardAgent yes

Host personal-git
  HostName centuria-guild.org
  Port 32323
  User git

Host centuria
  HostName centuria-guild.org
  Port 23232
  User darc
  ForwardAgent yes

Host aynrand2
  HostName aynrand2.org
  Port 32323
  User darc
  ForwardAgent yes

Host jupiter
  Port 9236
  User jupiter
  HostName jupiter.nkpadmin.com
  ForwardAgent yes

Host git-codecommit.us-west-1.amazonaws.com
  User APKAJP7KDRJLVTNFYDPQ

Host staging
  User ec2-user
  HostName stagessh.theofficeexpress.com
  ForwardAgent yes

Host phoenix
  User ec2-user
  HostName phxssh.theofficeexpress.com
  ForwardAgent yes
