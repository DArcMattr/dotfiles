{
  "name": "darceneaux/dotfiles",
  "description": "David Arceneaux global php utilities",
  "license": "MIT",
  "config": {
		"platform": {
			"php": "8.1"
		},
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "type": "library",
  "require": {
    "fig-r/psr2r-sniffer": "^1.3",
    "phpcompatibility/php-compatibility": "@dev",
    "phpmd/phpmd": "@stable",
    "phpstan/phpstan": "^1.7",
    "phpunit/phpunit": "@dev",
    "psy/psysh": "@dev",
    "sloanlance/pman-php-manual": "@stable",
    "squizlabs/php_codesniffer": "@dev",
    "tacnoman/dephpugger": "@dev",
    "vimeo/psalm": "@stable",
    "wp-cli/wp-cli-bundle": "@dev",
    "wp-coding-standards/wpcs": "@stable"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "optimize-autoloader": true
}
