{
  "name": "darceneaux/dotfiles",
  "description": "David Arceneaux global php utilities",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "type": "library",
  "autoload": {
    "classmap": [
      "scripts"
    ]
  },
  "require": {
    "fig-r/psr2r-sniffer": "@stable",
    "phpmd/phpmd": "@stable",
    "phpstan/phpstan": "@dev",
    "phpunit/phpunit": "@stable",
    "psy/psysh": "@dev",
    "slevomat/coding-standard": "@stable",
    "sloanlance/pman-php-manual": "@stable",
    "squizlabs/php_codesniffer": "@dev",
    "wp-cli/wp-cli-bundle": "@stable",
    "wp-coding-standards/wpcs": "@stable"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "optimize-autoloader": true
}
